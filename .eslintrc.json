{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true,
      "jasmine": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "sourceType": "module"
  },
  "rules": {
      "no-console": "off",
      "indent": ["error", 2],
      "linebreak-style": "off",
      "no-unused-vars": ["error", { "args": "none" } ],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "valid-jsdoc": [
        "error",
        { 
          "requireParamDescription": false,
          "requireReturnDescription": false
        }
      ]
  }
}